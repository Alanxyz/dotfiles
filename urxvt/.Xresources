Xft.antialias: true

! special
*.foreground:   #d8caac
*.background:   #323d43
*.cursorColor:  #d8caac

! black
*.color0:       #868d80
*.color8:       #868d80

! red
*.color1:       #e68183
*.color9:       #e68183

! green
*.color2:       #a7c080
*.color10:      #a7c080

! yellow
*.color3:       #d9bb80
*.color11:      #d9bb80

! blue
*.color4:       #89beba
*.color12:      #89beba

! magenta
*.color5:       #d3a0bc
*.color13:      #d3a0bc

! cyan
*.color6:       #87c095
*.color14:      #87c095

! white
*.color7:       #d8caac
*.color15:      #d8caac

URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.saveline: 2048
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

URxvt.transparent: flase
URxvt.shading: 55
URxvt.blurRadius: 12

URxvt*font: 		xft:Julia Mono:antialias=true Term:size=12
URxvt*boldFont:		xft:Julia Mono:bold:size=12
URxvt*italicFont:		xft:Julia Mono:italic:size=12
URxvt*scrollBar: 	false
URxvt*cursorUnderline:	false
URxvt*cursorBlink:	true
URxvt*depth: 32
URxvt*lineSpace: 1
URxvt*letterSpace: .75
URxvt.internalBorder: 20

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

xterm*font:     *-fixed-*-*-*-18-*
