*.color15:      #ffffff


Xft.dpi:		96
Xft.hinting:		1
Xft.hintstyle:		hintlight
Xft.antialias:		1
Xft.rgba:		rgb

URxvt.font:             xft:Iosevka Term:style=Regular:size=18
URxvt.boldFont:         xft:Iosevka Term:style=Bold:size=18
URxvt.italicFont:	xft:Iosevka Term:style=Italic:size=18
URxvt.scrollBar:        false
URxvt.iso14755:         false
URxvt.internalBorder:   20
URxvt.letterSpace:      0

URxvt*cursorBlink:	true

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.perl-ext-common: "default, -searchable-scrollback"
